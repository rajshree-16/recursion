long long fact(int N){
        if(N==1){
            return 1;
        }
        return N * fact(N-1);
    }
    vector<long long> factorialNumbers(long long N)
    {
        // Write Your Code here
        int i=1;
        vector<long long> ans;
        
            while(fact(i)<=N){
                ans.push_back(fact(i));
                i++;
            }
            return ans;
    }
